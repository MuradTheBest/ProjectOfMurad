<androidx.constraintlayout.widget.ConstraintLayout xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/page_log_in_page_ek1"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context=".Log_In_Screen">


	<ImageView
		android:id="@+id/vector_ek1"
		android:layout_width="match_parent"
		android:layout_height="85dp"
		android:contentDescription="vector_ek1"
		android:src="@drawable/vector_ek1"
		app:layout_constraintBottom_toTopOf="@+id/ellipse_4"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.0" />

	<View
		android:id="@+id/ellipse_4"
		android:layout_width="150dp"
		android:layout_height="150dp"
		android:background="@drawable/ellipse_4_shape"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.171" />

	<TextView
		android:id="@+id/tv_log_in"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center"
		android:gravity="center"
		android:text="Log in"
		android:textColor="@color/sign_up_color"
		android:textSize="34sp"
		app:layout_constraintBottom_toBottomOf="@+id/ellipse_4"
		app:layout_constraintEnd_toEndOf="@+id/ellipse_4"
		app:layout_constraintStart_toStartOf="@+id/ellipse_4"
		app:layout_constraintTop_toTopOf="@+id/ellipse_4" />


	<View
		android:id="@+id/rectangle_1_ek4"
		android:layout_width="337dp"
		android:layout_height="45dp"
		android:layout_marginTop="20dp"
		android:alpha="0.48"
		android:background="@drawable/rectangle_1_ek4_shape"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/ellipse_4"
		app:layout_constraintVertical_bias="0.065" />

	<EditText
		android:id="@+id/et_email_address"
		android:layout_width="0dp"
		android:layout_marginHorizontal="10dp"
		android:layout_height="0dp"
		android:layout_marginVertical="5dp"
		android:background="@android:color/transparent"
		android:hint="E-mail address"
		android:textColor="@color/username_color"
		android:textColorHint="@color/username_color"
		android:textSize="22sp"
		android:inputType="textEmailAddress"
		android:drawableStart="@drawable/ic_baseline_email_24"
		android:drawablePadding="10dp"
		app:layout_constraintBottom_toBottomOf="@+id/rectangle_1_ek4"
		app:layout_constraintEnd_toEndOf="@+id/rectangle_1_ek4"
		app:layout_constraintStart_toStartOf="@+id/rectangle_1_ek4"
		app:layout_constraintTop_toTopOf="@+id/rectangle_1_ek4" />

	<View
		android:id="@+id/rectangle_1_ek5"
		android:layout_width="337dp"
		android:layout_height="45dp"
		android:layout_marginTop="24dp"
		android:alpha="0.48"
		android:background="@drawable/rectangle_1_ek5_shape"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="@+id/rectangle_1_ek4"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintStart_toStartOf="@+id/rectangle_1_ek4"
		app:layout_constraintTop_toBottomOf="@+id/rectangle_1_ek4"
		app:layout_constraintVertical_bias="0.0" />

	<EditText
		android:id="@+id/et_password"
		android:layout_width="0dp"
		android:layout_marginHorizontal="10dp"
		android:layout_height="0dp"
		android:layout_marginVertical="5dp"
		android:hint="Password"
		android:textColor="@color/username_color"
		android:textColorHint="@color/username_color"
		android:textSize="22sp"
		android:inputType="textPassword"
		android:background="@android:color/transparent"
		android:drawableStart="@drawable/ic_baseline_password_24"
		android:drawablePadding="10dp"
		app:layout_constraintBottom_toBottomOf="@+id/rectangle_1_ek5"
		app:layout_constraintEnd_toEndOf="@+id/rectangle_1_ek5"
		app:layout_constraintStart_toStartOf="@+id/rectangle_1_ek5"
		app:layout_constraintTop_toTopOf="@+id/rectangle_1_ek5" />

	<TextView
		android:id="@+id/tv_don_t_have_an_account"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center"
		android:gravity="center"
		android:text="Don't have an account?"
		android:textColor="@color/don_t_have_an_account__color"
		android:textSize="22sp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="@+id/rectangle_1_ek5"
		app:layout_constraintHorizontal_bias="0.495"
		app:layout_constraintStart_toStartOf="@+id/rectangle_1_ek5"
		app:layout_constraintTop_toBottomOf="@+id/btn_log_in"
		app:layout_constraintVertical_bias="0.108" />

	<TextView
		android:id="@+id/tv_sign_up_now"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal|center"
		android:layout_marginTop="10dp"
		android:gravity="center"
		android:text="Sign up now!"
		android:textColor="@color/username_color"
		android:textSize="22sp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="@+id/tv_don_t_have_an_account"
		app:layout_constraintHorizontal_bias="0.504"
		app:layout_constraintStart_toStartOf="@+id/tv_don_t_have_an_account"
		app:layout_constraintTop_toBottomOf="@+id/tv_don_t_have_an_account"
		app:layout_constraintVertical_bias="0.0" />

	<Button
		android:id="@+id/btn_log_in"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="30dp"
		android:text="Log in"
		android:textSize="16sp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="@+id/rectangle_1_ek5"
		app:layout_constraintStart_toStartOf="@+id/rectangle_1_ek5"
		app:layout_constraintTop_toBottomOf="@+id/rectangle_1_ek5"
		app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>